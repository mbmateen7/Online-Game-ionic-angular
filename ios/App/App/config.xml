<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="Contacts">
    <param name="ios-package" value="CDVContacts"/>
  </feature>

  <feature name="File">
    <param name="ios-package" value="CDVFile"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="GooglePlus">
    <param name="ios-package" value="GooglePlus"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="PaymentsPlugin">
    <param name="ios-package" value="PaymentsPlugin"/>
  </feature>

  <feature name="Media">
    <param name="ios-package" value="CDVSound"/>
  </feature>

  <feature name="SpinnerDialog">
    <param name="ios-package" value="CDVSpinnerDialog"/>
  </feature>

  <feature name="NativeAudio">
    <param name="ios-package" value="NativeAudio"/>
  </feature>

  <feature name="InAppPurchase">
    <param name="ios-package" value="InAppPurchase"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="SignInWithApple">
    <param name="ios-package" value="SignInWithApple"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="SocialSharing">
    <param name="ios-package" value="SocialSharing"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="Device">
    <param name="ios-package" value="CDVDevice"/>
  </feature>

  <feature name="FacebookConnectPlugin">
    <param name="ios-package" value="FacebookConnectPlugin"/>
    <param name="onload" value="true"/>
  </feature>

  
</widget>